<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-grandorgue-odf">
    <sub-class-of type="text/plain"/>
    <comment>GrandOrgue organ definition</comment>
    <glob pattern="*.organ"/>
    <icon name="GrandOrgue.png"/>
  </mime-type>
  <mime-type type="application/x-grandorgue-package">
    <sub-class-of type="text/plain"/>
    <comment>GrandOrgue organ package</comment>
    <glob pattern="*.orgue"/>
    <icon name="GrandOrgue.png"/>
  </mime-type>
  <mime-type type="application/x-grandorgue-cmb">
    <sub-class-of type="text/plain"/>
    <comment>GrandOrgue setting file</comment>
    <glob pattern="*.cmb"/>
    <icon name="GrandOrgue.png"/>
  </mime-type>
</mime-info>
